<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <title>Student</title>

</head>

<body>

    <section id="header">
        <!--navbar-->
        <header>
            <nav class="navbar navbar-expand-lg navbar-light bg-light">
                <div class="container-fluid">
                    <a class="navbar-brand" href="/"><img style="width: 150px;" src="/images/LOGO.png" alt=""></a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                        data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                            <li class="nav-item">
                                <a class="nav-link active" aria-current="page" href="/signout">Sign out</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </header>
    </section>
    <section>
        <% if (successMessage && successMessage.length> 0) { %>
            <div class="alert alert-success">
                <%= successMessage %>
            </div>
            <% } %>

    </section>
    <!--Body-->
    <div class="container-fluid">
        <div class="row">
            <div class="col-12 col-md-2">
                <div class="row h-100">
                    <div class="col-12 menu-container">
                        <nav class="nav flex-column">
                            <a class="nav-link active" aria-current="page" href="/scheduleAppointment">Set
                                Appointment</a>
                            <a class="nav-link" href="/availableTimeslots">View Available Time slots</a>
                            <a class="nav-link" href="#">Cancel Appointments</a>
                            <a class="nav-link active" aria-current="page" href="/searchAppointment">Search
                                Appointment</a>
                            <a class="nav-link" href="/Settings">Settings</a>
                            <a class="nav-link" href="/student-cancelled-appointments">Cancelled Appointments</a>
                            <a class="nav-link" href="/logs">User logs</a>

                        </nav>
                    </div>
                </div>
            </div>
            <div class="col-12 col-md-10">
                <section id="dashboard-heading">
                    <h1>Logs dashboard</h1>
                </section>
                
            </div>
        </div>
    </div>


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
        crossorigin="anonymous"></script>
    <script src="../scripts/hideflashmessage.js"></script>
</body>

</html>